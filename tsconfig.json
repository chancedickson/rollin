{
  "compilerOptions": {
    "target": "ES2017",            
    "module": "ESNext",            
    "jsx": "react-native",         
    "noEmit": true,
    "sourceMap": true,             
    "removeComments": true,        
    "strict": true,                
    "noUnusedLocals": true,        
    "noUnusedParameters": true,    
    "moduleResolution": "node"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
